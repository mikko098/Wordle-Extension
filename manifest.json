{
    "manifest_version" : 3,
    "name" : "Wordle extension",
    "description" : "extension to cheat at wordle",
    "version" : "1.0",
    "action" : {
    },
    "web_accessible_resources" : [
        {"resources": ["src/code/solutions.txt"],
        "matches": ["<all_urls>"]}
    ],
    "background" : {
        "service_worker" : "src/code/index.js",
        "type" : "module"
    },
    "permissions" : ["scripting", "activeTab"]
}